generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users {
  id                String   @id @default(uuid())
  emailAddress      String
  name              String
  gender            String
  sleepTimeDuration Int      @default(0)
  createdAt         DateTime @default(now())
}
